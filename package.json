    "sass": "^1.54.2",
    "sass-loader": "^13.0.2",
    "string-replace-loader": "^3.1.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-manifest-plugin": "^5.0.0"
  }
